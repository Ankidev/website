                <div class="navbar-dropdown is-hidden-mobile">
                    <a class="navbar-item" href="<%= discordServer %>">
                        <span class="icon iconalign">
                            <i class="fab fa-discord" aria-hidden="true"></i>
                        </span>
                        <span><%= __("common.nav.more.discord") %></span>
                    </a>
                    <a class="navbar-item" href="https://docs.discordextremelist.xyz/">
