                        <a class="navbar-item" href=">%= linkPrefix %>/users/account/preferences">
                            <span class="icon iconalign">
                                <i class="fad fa-swatchbook" aria-hidden="true"></i>
                            </span>
                            <span><%= __("common.nav.me.preferences") %></span>
                        </a>
                        <a class="navbar-item" href="<%= linkPrefix %>/bots/submit">
                            <span class="icon iconalign">
                                <i class="fad fa-robot" aria-hidden="true"></i>
                            </span>
