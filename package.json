{
  "name": "del",
  "version": "5.0.0",
  "author": "Cairo Mitchell-Acason",
  "private": true,
  "scripts": {
    "start": "node scripts/start",
    "pm2": "pm2 start scripts/start --name 'DEL'",
    "dev": "nodemon scripts/start",
    "css-build": "node-sass --omit-source-map-url src/Assets/Sass/main.scss src/Assets/Public/css/standard.css --output-style compressed && node-sass --omit-source-map-url src/Assets/Sass/form.scss src/Assets/Public/css/form.css --output-style compressed && node-sass --omit-source-map-url src/Assets/Sass/all.scss src/Assets/Public/css/all.css --output-style compressed",
    "css-watch": "npm run css-build -- --watch",
    "editor-build": "cp -r ./node_modules/monaco-editor/ ./src/Assets/Public/packages/monaco-editor"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "browser-detect": "^0.2.28",
    "child_process": "^1.0.2",
    "chunk": "0.0.2",
    "connect-redis": "^4.0.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "eris": "^0.11.2",
    "eris-additions": "^1.3.0",
    "express": "~4.16.1",
    "express-device": "^0.4.2",
    "express-session": "^1.17.0",
    "html-entities": "^1.2.1",
    "http-errors": "~1.6.3",
    "i18n": "^0.8.5",
    "ioredis": "^4.16.0",
    "marked": "^0.8.0",
    "mongodb": "^3.5.2",
    "morgan": "~1.9.1",
    "node-cron": "^2.0.3",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.3",
    "redis": "^2.8.0",
    "request": "^2.88.2",
    "sanitize-html": "^1.22.0",
    "snekfetch": "^4.0.4"
  },
  "devDependencies": {
    "bulma": "^0.8.0",
    "bulma-extensions": "^6.2.7",
    "compression": "^1.7.4",
    "monaco-editor": "^0.20.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2"
  }
}
